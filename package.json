{
  "name": "all-the-tools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "netlify:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider netlify dev",
    "start:network": "ng serve --host 0.0.0.0",
    "build": "npm run generate:seo && ng build && npm run generate:sitemap && npm run generate:rss",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng serve",
    "build:ssr": "npm run generate:seo && ng build && npm run generate:sitemap && npm run generate:rss",
    "prerender": "npm run generate:seo && ng build --configuration production && npm run generate:sitemap && npm run generate:rss",
    "serve:ssr:all-the-tools": "node dist/all-the-tools/server/server.mjs",
    "generate:seo": "node scripts/generate-seo-files.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:rss": "node tools/generate-rss.js",
    "calculate:readtimes": "node scripts/calculate-read-times.js",
    "compose:server": "node scripts/compose-server.js"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.14",
    "@angular/build": "^20.0.4",
    "@angular/cdk": "^20.2.14",
    "@angular/cli": "^20.0.4",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.2.14",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-server": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@angular/ssr": "^20.3.12",
    "@netlify/angular-runtime": "^3.0.1",
    "@netlify/functions": "^2.8.2",
    "@types/express": "^5.0.5",
    "@types/qrcode": "^1.5.6",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "cheerio": "^1.0.0",
    "express": "^5.1.0",
    "get-ascii-image": "^1.3.6",
    "googleapis": "^144.0.0",
    "jszip": "^3.10.1",
    "matter-js": "^0.20.0",
    "nodemailer": "^7.0.11",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
    "tesseract.js": "^5.1.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@types/jasmine": "~5.1.0",
    "@types/matter-js": "^0.20.2",
    "@types/node": "^20.17.19",
    "cross-env": "^10.1.0",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0"
  }
}
